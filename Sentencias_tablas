-- MySql y Mariadb
CREATE TABLE inventario (
  id INT PRIMARY KEY AUTO_INCREMENT,
  libro VARCHAR(255),
  cuaderno VARCHAR(255)
);

-- PostgreSQL
CREATE TABLE inventario (
  id INT PRIMARY KEY,
  libro VARCHAR(255),
  cuaderno VARCHAR(255)
);

-- SQLITE
CREATE TABLE inventario (
  id INTEGER PRIMARY KEY,
  libro TEXT,
  cuaderno TEXT
);
